<table class="table table-hover">
  <thead>
    <tr>
      <th>name</th>
      <th>address</th>
      <th>location</th>
      <th>phone</th>
      <th>website</th>
      <th>email</th>
    </tr>
  </thead>
  <tbody>
    <% @merchants.each do |m| %>
      <tr>
        <td><%= m.name %></td>
        <td><%= m.address %></td>
        <td><%= m.location %></td>
        <td><%= m.phone %></td>
        <td><%= m.website %></td>
        <td><%= m.email %></td>
      </tr>
    <% end %>
</table>
